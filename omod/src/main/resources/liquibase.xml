	<changeSet id="openhmis.cashier-v1.7.0-1" author="ibewes">
		<comment>Remove all the rounding item prices</comment>
		<sql>
			DELETE cp.*
			FROM cashier_item_price cp INNER JOIN cashier_item i ON
					cp.item_id = i.item_id
				INNER JOIN cashier_department d ON
					i.department_id = d.department_id
			WHERE i.name = 'Rounding' AND d.name = 'Rounding'
		</sql>

		<comment>Create a single rounding item price</comment>
		<sql>
			INSERT INTO cashier_item_price (item_id, price, name, description, creator, date_created, uuid)
			SELECT i.item_id, 0.0, '', '', 1, NOW(), UUID()
			FROM cashier_item i INNER JOIN cashier_department d ON
			i.department_id = d.department_id
			WHERE i.name = 'Rounding' AND d.name = 'Rounding'
		</sql>

		<comment>Update the default price id on the rounding item</comment>
		<sql>
			UPDATE cashier_item i INNER JOIN cashier_department d ON
			i.department_id = d.department_id
			INNER JOIN cashier_item_price cp ON
			i.item_id = cp.item_id
			SET i.default_price_id = cp.item_price_id
			WHERE i.name = 'Rounding' AND d.name = 'Rounding'
		</sql>
	</changeSet>
	<changeSet id="openhmis.cashier-v2.0-0" author="ibewes">
		<preConditions onFail="MARK_RAN">
			<and>
				<tableExists tableName="inv_item" />
				<sqlCheck expectedResult="0">
					SELECT COUNT(*) FROM inv_item
				</sqlCheck>
			</and>
		</preConditions>
		<comment>
			This script copies the previously defined cashier departments and items (with associated child tables)
			into the appropriate inventory module tables.  This script will only be run if the inventory module has been
			installed but no items have been created.
		</comment>
		<sql>
			INSERT INTO inv_department (department_id, name, description,
			creator, date_created, changed_by, date_changed, retired, retired_by, date_retired,
			retire_reason, uuid)
			SELECT department_id, name, description,
			creator, date_created, changed_by, date_changed, retired, retired_by, date_retired,
			retire_reason, uuid
			FROM cashier_department;

			INSERT INTO inv_item (item_id, name, description, department_id, default_price_id,
			creator, date_created, changed_by, date_changed, retired, retired_by, date_retired,
			retire_reason, uuid)
			SELECT item_id, name, description, department_id, default_price_id,
			creator, date_created, changed_by, date_changed, retired, retired_by, date_retired,
			retire_reason, uuid
			FROM cashier_item;

			INSERT INTO inv_item_code (item_code_id, item_id, code, name, description,
			creator, date_created, changed_by, date_changed, retired, retired_by, date_retired,
			retire_reason, uuid)
			SELECT item_code_id, item_id, code, name, description, creator, date_created, changed_by, date_changed, retired, retired_by, date_retired,
			retire_reason, uuid
			FROM cashier_item_code;

			INSERT INTO inv_item_price (item_price_id, item_id, price, name, description,
			creator, date_created, changed_by, date_changed, retired, retired_by, date_retired,
			retire_reason, uuid)
			SELECT item_price_id, item_id, price, name, description, creator, date_created, changed_by, date_changed, retired, retired_by, date_retired,
			retire_reason, uuid
			FROM cashier_item_price;
		</sql>
	</changeSet>
