	<parameter name="timesheetId" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    d.name AS department_name,
    SUM(li.quantity) AS department_quantity,
    SUM(li.price) AS department_amount
FROM
    cashier_bill b INNER JOIN cashier_timesheet t ON
        b.provider_id = t.provider_id AND
        b.date_created >= t.clock_in AND
        (t.clock_out IS NULL OR b.date_created < t.clock_out)
    INNER JOIN cashier_bill_line_item li ON
        b.bill_id = li.bill_id
    INNER JOIN cashier_item i ON
       li.item_id = i.item_id
    INNER JOIN cashier_department d ON
        i.department_id = d.department_id
WHERE
    t.timesheet_id = 10
GROUP BY d.name]]>
	</queryString>
	<field name="department_name" class="java.lang.String"/>
	<field name="department_quantity" class="java.math.BigDecimal"/>
	<field name="department_amount" class="java.math.BigDecimal"/>
	<variable name="totalAmount" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{department_amount}.multiply($F{department_quantity})]]></variableExpression>
		<initialValueExpression><![CDATA[0]]></initialValueExpression>
	</variable>
